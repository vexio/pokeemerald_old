script TriadGorge_Object_RockSmashHiker {
	lock
	faceplayer
	msgbox(format("{COLOR BLUE}These rocks are lookin' like they're blocking the way to {COLOR GREEN}Myst City{COLOR BLUE}.\pIf I had the {COLOR GREEN}Fiery Badge{COLOR BLUE} I could smash these rocks!"))
	if (flag(FLAG_BADGE03_GET) == true) {
		applymovement(14, Common_Movement_ExclamationMark)
		waitmovement(0)
		playse(SE_PIN)
		waitse
		msgbox(format("{COLOR BLUE}Is that the {COLOR GREEN}Fiery Badge{COLOR BLUE}?\pWowza!\nYou must be a strong trainer!\pHere, take this!"))
		giveitem(ITEM_HM06_ROCK_SMASH, 1)
		msgbox(format("{COLOR BLUE}With that HM, you can remove these rocks and similar looking ones without a problem!\pI'm going to go get myself the {COLOR GREEN}Fiery Badge{COLOR BLUE} as well so I can use Rock Smash too!"), MSGBOX_AUTOCLOSE)
		delay(10)
		getplayerxy(VAR_TEMP_0, VAR_TEMP_1)
		if (var(VAR_TEMP_0) == 66) {
			applymovement(14, TriadGorge_RockSmashHiker_Movement_1)
			waitmovement(0)
		} else {
			applymovement(14, TriadGorge_RockSmashHiker_Movement_0)
			waitmovement(0)
		}
		removeobject(14)
		setflag(FLAG_TRIADGORGE_ROCKSMASH)
		release
		end
	} else {
		closemessage
		release
		end
	}
}

movement TriadGorge_RockSmashHiker_Movement_0 {
	walk_left * 7
	walk_up * 7
}
movement TriadGorge_RockSmashHiker_Movement_1 {
	walk_up
	walk_left * 7
	walk_up * 6
}