	msgbox Route106_Text_ElliotRematchPostBattle, MSGBOX_AUTOCLOSE
	end

Route106_EventScript_Ned:: @ 81EE523
	trainerbattle_single TRAINER_NED, Route106_Text_NedIntro, Route106_Text_NedDefeated
	msgbox Route106_Text_NedPostBattle, MSGBOX_AUTOCLOSE
	end

Route106_Text_TrainerTips: @ 81EE53A
	.string "TRAINER TIPS\p"
	.string "Advice on catching POKéMON with a ROD:\n"
	.string "Press the A Button if you get a bite.$"


`

const PLAYER = OBJ_EVENT_ID_PLAYER
const GRUNT_1 = 5 // male
const GRUNT_2 = 4 // male w/ accent
const GRUNT_3 = 3 // female
const HAUMEA = 2
const CAMERA = OBJ_EVENT_ID_CAMERA
const SKIP = 15

script TriadGorge_Trigger_ProtoGrunts {
    lockall
    fadescreen(FADE_TO_BLACK)
    cleartrainerflag(TRAINER_GRUNT_TRIAD_GORGE_1)
    cleartrainerflag(TRAINER_GRUNT_TRIAD_GORGE_2)
    cleartrainerflag(TRAINER_GRUNT_TRIAD_GORGE_3)
    cleartrainerflag(TRAINER_ADMIN_HAUMEA_1)
    getplayerxy(VAR_TEMP_0, VAR_TEMP_1)
    if (var(VAR_TEMP_1) == 20) {
        applymovement(PLAYER, TriadGorge_ProtoGrunts_Movement_0_0)
        waitmovement(0)
    } elif(var(VAR_TEMP_1) == 22) {
		applymovement(PLAYER, TriadGorge_ProtoGrunts_Movement_0_1)
        waitmovement(0)
	} elif(var(VAR_TEMP_1) == 23) {
		applymovement(PLAYER, TriadGorge_ProtoGrunts_Movement_0_2)
        waitmovement(0)
	}
	delay(5)
	special(SpawnCameraObject)
	applymovement(CAMERA, TriadGorge_ProtoGrunts_CMovement_0)
	waitmovement(0)
	playbgm(MUS_AJITO, 0)
	fadescreen(FADE_FROM_BLACK)
	delay(10)
	applymovement(HAUMEA, Common_Movement_WalkInPlaceLeft)
	waitmovement(0)
	msgbox(format("Haumea: {COLOR BLUE}You three!\nWhere have you been?"))
	applymovement(GRUNT_3, Common_Movement_WalkInPlaceRight)
	waitmovement(0)
	msgbox(format("Grunt 3: {COLOR RED}Sorry {COLOR GREEN}Haumea{COLOR RED}, won't happen again!"))
	applymovement(GRUNT_3, Common_Movement_WalkInPlaceUp)
	waitmovement(0)
	msgbox(format("{COLOR RED}Right boys?"), MSGBOX_AUTOCLOSE)
	delay(10)
	applymovement(GRUNT_1, Common_Movement_JumpInPlaceRight)
	applymovement(GRUNT_2, Common_Movement_JumpInPlaceRight)
	playse(SE_JITE_PYOKO)
	waitmovement(0)
	applymovement(GRUNT_1, Common_Movement_WalkInPlaceRight)
	applymovement(GRUNT_2, Common_Movement_WalkInPlaceRight)
	waitmovement(0)
	applymovement(GRUNT_1, Common_Movement_ExclamationMark)
	applymovement(GRUNT_2, Common_Movement_ExclamationMark)
	waitmovement(0)
	playse(SE_PIN)
	delay(16)
	msgbox(format("Grunt 1: {COLOR BLUE}Yes {COLOR GREEN}Haumea{COLOR BLUE} sir!\n{COLOR DARK_GREY}Grunt 2: {COLOR BLUE}Yes {COLOR GREEN}Haumea{COLOR BLUE} sir!"), MSGBOX_AUTOCLOSE)
	delay(10)
	applymovement(GRUNT_3, Common_Movement_WalkInPlaceRight)
	applymovement(HAUMEA, Common_Movement_WalkInPlaceLeft)
	waitmovement(0)
	msgbox(format("Haumea: {COLOR BLUE}Good.\pNow, do you guys have those dusty old books?"), MSGBOX_AUTOCLOSE)
	delay(5)
	applymovement(GRUNT_2, Common_Movement_QuestionMark)
	waitmovement(0)
	playse(SE_PIN)
	delay(16)
	msgbox(format("Grunt 2: {COLOR BLUE}Uhhh...\nBooks? As in multipl'?"))
	applymovement(HAUMEA, Common_Movement_WalkInPlaceLeft)
	waitmovement(0)
	msgbox(format("Haumea: {COLOR BLUE}Yes as in multiple!\pWe need the {COLOR GREEN}Dorimtome{COLOR BLUE} and the {COLOR GREEN}Akumutome{COLOR BLUE}!"))
	applymovement(GRUNT_2, Common_Movement_WalkInPlaceRight)
	waitmovement(0)
	msgbox(format("Grunt 2: {COLOR BLUE}...\nWhic' one do I hav' here?"))
	applymovement(GRUNT_3, Common_Movement_WalkInPlaceUp)
	waitmovement(0)
	msgbox(format("Grunt 3: {COLOR RED}Let me see that!"), MSGBOX_AUTOCLOSE)
	applymovement(GRUNT_2, Common_Movement_WalkInPlaceDown)
	waitmovement(0)
	delay(5)
	applymovement(GRUNT_3, Common_Movement_WalkInPlaceUp)
	waitmovement(0)
	delay(20)
	msgbox(format("Grunt 3: {COLOR RED}...\pIt looks like it's the {COLOR GREEN}Akumutome{COLOR BLUE}."))
	msgbox(format("Grunt 2: {COLOR BLUE}That's th' one I got from {COLOR GREEN}Alnerwick Library{COLOR BLUE}."))
	applymovement(GRUNT_2, Common_Movement_WalkInPlaceRight)
	applymovement(GRUNT_3, Common_Movement_WalkInPlaceRight)
	waitmovement(0)
	msgbox(format("Haumea: {COLOR BLUE}Did anyone see you steal that book?\p{COLOR DARK_GREY}Grunt 2: {COLOR BLUE}Only the librarian and some kid.\p{COLOR DARK_GREY}Haumea: {COLOR BLUE}Good.\nNow you three need to go get that {COLOR GREEN}Dorimtome{COLOR BLUE} in {COLOR GREEN}Enclava Tower{COLOR BLUE}!"))
	applymovement(GRUNT_1, Common_Movement_WalkInPlaceRight)
	applymovement(GRUNT_2, Common_Movement_WalkInPlaceRight)
	applymovement(GRUNT_3, Common_Movement_WalkInPlaceRight)
	waitmovement(0)
	msgbox(format("Grunts: {COLOR BLUE}Yes sir!"), MSGBOX_AUTOCLOSE)
	delay(15)
	applymovement(GRUNT_1, Common_Movement_WalkInPlaceLeft)
	applymovement(GRUNT_2, Common_Movement_WalkInPlaceLeft)
	applymovement(GRUNT_3, Common_Movement_WalkInPlaceLeft)
	waitmovement(0)
	delay(10)
	applymovement(GRUNT_1, Common_Movement_ExclamationMark)
	applymovement(GRUNT_2, Common_Movement_ExclamationMark)
	applymovement(GRUNT_3, Common_Movement_ExclamationMark)
	waitmovement(0)
	playse(SE_PIN)
	delay(16)
	applymovement(GRUNT_2, Common_Movement_JumpInPlaceLeft)
	waitmovement(0)
	playse(SE_JITE_PYOKO)
	delay(5)
	msgbox(format("Grunt 2: {COLOR BLUE}That's th' kid from {COLOR GREEN}Alnerwick Library{COLOR BLUE}!"))
	applymovement(HAUMEA, Common_Movement_WalkInPlaceLeft)
	waitmovement(0)
	msgbox(format("Haumea: {COLOR BLUE}So you've been eaves dropping, eh kid?\pGrunts!\nTake care of that nuisance!\pI don't plan on getting my hands dirty."), MSGBOX_AUTOCLOSE)
	delay(10)
	applymovement(CAMERA, TriadGorge_ProtoGrunts_CMovement_1)
	waitmovement(0)
	special(RemoveCameraObject)
	delay(10)
	applymovement(GRUNT_1, TriadGorge_ProtoGrunts_Movement_1)
	waitmovement(0)
	applymovement(PLAYER, Common_Movement_WalkInPlaceUp)
	waitmovement(0)
	msgbox(format("Grunt 1: {COLOR BLUE}You're gonna regret messing with {COLOR GREEN}Team Proto{COLOR BLUE}!"))
	trainerbattle_no_intro(TRAINER_GRUNT_TRIAD_GORGE_1, format("Gah!"))
	playbgm(MUS_AJITO, 0)
	delay(5)
	applymovement(GRUNT_2, TriadGorge_ProtoGrunts_Movement_2)
	waitmovement(0)
	applymovement(PLAYER, Common_Movement_WalkInPlaceRight)
	waitmovement(0)
	msgbox(format("Grunt 2: {COLOR BLUE}You're th' kid from {COLOR GREEN}Alnerwick Library{COLOR BLUE}eh?\pStay outta our busines'!"))
	trainerbattle_no_intro(TRAINER_GRUNT_TRIAD_GORGE_2, format("Stay outta it kid!"))
	playbgm(MUS_AJITO, 0)
	delay(5)
	applymovement(GRUNT_3, TriadGorge_ProtoGrunts_Movement_3)
	waitmovement(0)
	applymovement(PLAYER, Common_Movement_WalkInPlaceDown)
	waitmovement(0)
	msgbox(format("Grunt 3: {COLOR RED}Those two bafoons don't know how to represent {COLOR GREEN}Team Proto{COLOR RED}!"))
	trainerbattle_no_intro(TRAINER_GRUNT_TRIAD_GORGE_3, format("Running out of energy?"))
	playbgm(MUS_AJITO, 0)
	delay(5)
	applymovement(GRUNT_2, TriadGorge_ProtoGrunts_Movement_4)
	waitmovement(0)
	applymovement(PLAYER, Common_Movement_WalkInPlaceRight)
	applymovement(HAUMEA, TriadGorge_ProtoGrunts_Movement_5)
	waitmovement(0)
	msgbox(format("Haumea: {COLOR BLUE}These idiots don't know how to do anything right!\pListen, kid.\nI know you're trying to be the hero, and trying to get that book back to {COLOR GREEN}Alnerwick Library{COLOR BLUE}, but that ideology won't work here.\pNow, time to teach you a valuable lesson here, kid!"))
	trainerbattle_no_intro(TRAINER_ADMIN_HAUMEA_1, format("What?\pA kid beat me?"))
	playbgm(MUS_AJITO, 0)
	delay(5)
	applymovement(HAUMEA, Common_Movement_WalkInPlaceLeft)
	waitmovement(0)
	msgbox(format("Haumea: {COLOR BLUE}You're a strong trainer, I can see that...\pBut {COLOR GREEN}Team Proto{COLOR BLUE} won't be stopped by a child!\pThis won't be the last you see of {COLOR GREEN}Team Proto{COLOR BLUE}!\pAbra, use Teleport!"))
	delay(10)
	playmoncry(SPECIES_ABRA, 0)
	waitmoncry
	delay(10)
	fadescreen(FADE_TO_BLACK)
	closemessage
	setvar(VAR_PROTOGRUNTS_ENCLAVAGORGE, 3)
	removeobject(GRUNT_1)
	removeobject(GRUNT_2)
	removeobject(GRUNT_3)
	removeobject(HAUMEA)
	playse(SE_TK_WARPOUT)
	setobjectxy(SKIP, 36, 21)
	delay(15)
	fadescreen(FADE_FROM_BLACK)
	delay(20)
	applymovement(SKIP, TriadGorge_ProtoGrunts_Movement_6)
	waitmovement(0)
	applymovement(SKIP, Common_Movement_ExclamationMark)
	waitmovement(0)
	playse(SE_PIN)
	waitse
	playbgm(MUS_RG_RIVAL1, 0)
	msgbox(format("Skip: {COLOR BLUE}There you are slowpoke!"), MSGBOX_AUTOCLOSE)
	applymovement(SKIP, TriadGorge_ProtoGrunts_Movement_7)
	waitmovement(0)
	msgbox(format("Skip: {COLOR BLUE}Did you just deal with those robed people?\pThey looked weak, I'm surprised you stood a chance against them!\pI've been training for the {COLOR GREEN}Fiery Badge{COLOR BLUE} up north here on {COLOR GREEN}Frostpine Climb{COLOR BLUE}, and now my team is stronger than ever!\pI'm not gonna take it out on you, since you and your team just took a beating from those {COLOR GREEN}Team Proto{COLOR BLUE} goons.\pI also found this HM, I don't think I'm going to have any use for it."))
	giveitem(ITEM_HM01_CUT, 1)
	msgbox(format("Skip: {COLOR BLUE}Anyways, I'm gonna head up north to get the third badge!\pI'm going to continue training until I become the strongest trainer in all of Ryoko!"), MSGBOX_AUTOCLOSE)
	applymovement(SKIP, TriadGorge_ProtoGrunts_Movement_8)
	playbgm(MUS_NEXTROAD, 0)
	waitmovement(0)
	removeobject(SKIP)
	setflag(FLAG_TRIAD_GORGE_GRUNTS)
	release
	end
}
movement TriadGorge_ProtoGrunts_Movement_0_0 {
    walk_fastest_down
    walk_in_place_fastest_right
}
movement TriadGorge_ProtoGrunts_Movement_0_1 {
    walk_fastest_up
    walk_in_place_fastest_right
}
movement TriadGorge_ProtoGrunts_Movement_0_2 {
    walk_fastest_up * 2
    walk_in_place_fastest_right
}
movement TriadGorge_ProtoGrunts_CMovement_0 {
	walk_fastest_right * 5
}
movement TriadGorge_ProtoGrunts_CMovement_1 {
	walk_left * 5
}
movement TriadGorge_ProtoGrunts_Movement_1 {
    walk_left * 2
    walk_in_place_down
}
movement TriadGorge_ProtoGrunts_Movement_2 {
    walk_left
}
movement TriadGorge_ProtoGrunts_Movement_3 {
    walk_left * 2
	walk_in_place_up
}
movement TriadGorge_ProtoGrunts_Movement_4 {
    walk_up
	walk_in_place_down
}
movement TriadGorge_ProtoGrunts_Movement_5 {
    walk_left * 4
}

movement TriadGorge_ProtoGrunts_Movement_6 {
	walk_left * 4
}
movement TriadGorge_ProtoGrunts_Movement_7 {
	walk_left * 3
}
movement TriadGorge_ProtoGrunts_Movement_8 {
	walk_right * 8
}

